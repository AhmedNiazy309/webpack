{
    "extends": "eslint:recommended",
    "parser": "babel-eslint",
    "rules": {
        "no-console": ["off"]
      },
      "parserOptions": {
        "ecmaVersion": 9
      },
      "env": {
        "es6": true,
        "node": true,
        "browser": true,
        "amd": true
      },
      "globals": {
        "$": true,
        "require": true,
        "process": true
      },
      "root": true
}
